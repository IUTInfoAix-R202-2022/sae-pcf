<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="700.0" prefWidth="1200" styleClass="transparentBG" type="javafx.scene.control.ScrollPane" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox alignment="TOP_CENTER" spacing="50.0">
         <children>
            <Label fx:id="allFieldsFill" alignment="CENTER" styleClass="bigbold" text="Tout les champs avec la mention * doivent être remplis">
               <font>
                  <Font name="JetBrains Mono Regular" size="13.0" />
               </font>
            </Label>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5, 0.5">
                     <items>
                        <Label fx:id="themeOfUseLabel" prefWidth="300.0" text="Thématique d'usage *">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="choiceBoxThemeOfUse" prefWidth="600.0" />
                        <Button fx:id="addButtonThemeOfUse" mnemonicParsing="false" onAction="#addAThemeOfUse" text="+">
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </Button>
                     </items>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </SplitPane>
                  <Label fx:id="labelNotFillThemeOfUse" styleClass="error" text="Veuillez remplir ce champ" visible="false" />
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5, 0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="disciplineLabel" prefWidth="300.0" text="Discipline *">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="choiceBoxDiscipline" prefWidth="600.0" />
                        <Button fx:id="addButtonDiscipline" mnemonicParsing="false" onAction="#addADiscipline" text="+" />
                     </items>
                  </SplitPane>
                  <Label fx:id="labelNotFillDiscipline" styleClass="error" text="Veuillez remplir ce champ" visible="false" />
              </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5, 0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="degreeLabel" prefWidth="300.0" text="Degré *">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="choiceBoxDegree" prefWidth="600.0" />
                        <Button fx:id="addButtonDegree" mnemonicParsing="false" onAction="#addADegree" text="+" />
                     </items>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </SplitPane>
                  <Label fx:id="labelNotFillDegree" styleClass="error" text="Veuillez remplir ce champ" visible="false" />
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5, 0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="academyLabel" prefWidth="300.0" text="Académie *">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="choiceBoxAcademy" prefWidth="600.0" />
                        <Button fx:id="addButtonAcademy" mnemonicParsing="false" onAction="#addAnAcademy" text="+" />
                     </items>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </SplitPane>
                  <Label fx:id="labelNotFillAcademy" styleClass="error" text="Veuillez remplir ce champ" visible="false" />
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5, 0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="academicRegionLabel" prefWidth="300.0" text="Région académique">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <ChoiceBox fx:id="choiceBoxAcademicRegion" prefWidth="600.0" />
                        <Button fx:id="addButtonAcademicRegion" mnemonicParsing="false" onAction="#addAAcademicRegion" text="+" />
                     </items>
                     <VBox.margin>
                        <Insets bottom="18.0" />
                     </VBox.margin>
                  </SplitPane>
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="actorTypeLabel" prefWidth="300.0" text="Type d'acteurs *">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="fieldTypeOfActors" />
                     </items>
                  </SplitPane>
                  <Label fx:id="labelNotFillTypeOfActors" styleClass="error" text="Veuillez remplir ce champ" visible="false" />
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="actorIdentityLabel" prefWidth="300.0" text="Identité des acteurs *">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <VBox fx:id="vBoxIdOfActor" spacing="10.0">
                           <children>
                              <HBox spacing="20.0">
                                 <children>
                                    <TextField fx:id="fieldIdOfActorName" promptText="Nom" HBox.hgrow="ALWAYS" />
                                 </children>
                              </HBox>
                              <HBox spacing="148.0">
                                 <children>
                                    <Label fx:id="labelNotFillIdOfActorName" styleClass="error" text="Veuillez remplir ce champ" visible="false">
                                       <HBox.margin>
                                          <Insets />
                                       </HBox.margin>
                                    </Label>
                                 </children>
                              </HBox>
                              <Button fx:id="buttonAddIdOfActor" mnemonicParsing="false" onAction="#addIdOfActor" text="Ajouter un acteur" />
                              <Button fx:id="removeButton" mnemonicParsing="false" onAction="#removeAnActor" text="Supprimer un acteur" />
                           </children>
                           <opaqueInsets>
                              <Insets />
                           </opaqueInsets>
                        </VBox>
                     </items>
                  </SplitPane>
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox styleClass="transparentBG">
               <children>
                  <SplitPane dividerPositions="0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="resourceLinkLabel" prefWidth="300.0" text="Lien de la ressource *">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="fieldResourceLink" />
                     </items>
                  </SplitPane>
                  <Label fx:id="labelNotFillResourceLink" styleClass="error" text="Veuillez remplir ce champ" visible="false" />
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="resourceNameLabel" prefWidth="300.0" text="Nom de la ressource">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="fieldResourceName" />
                     </items>
                     <VBox.margin>
                        <Insets bottom="18.0" />
                     </VBox.margin>
                  </SplitPane>
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="sourceTypeLabel" prefWidth="300.0" text="Type de source">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <TextField fx:id="fieldSourceType" />
                     </items>
                     <VBox.margin>
                        <Insets bottom="18.0" />
                     </VBox.margin>
                  </SplitPane>
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <VBox>
               <children>
                  <SplitPane dividerPositions="0.5" prefWidth="900.0">
                     <items>
                        <Label fx:id="commentariesLabel" alignment="TOP_LEFT" prefWidth="300.0" text="Commentaires">
                           <padding>
                              <Insets left="5.0" right="50.0" />
                           </padding>
                           <font>
                              <Font name="JetBrains Mono Regular" size="14.0" />
                           </font>
                        </Label>
                        <TextArea fx:id="fieldComment" />
                     </items>
                     <VBox.margin>
                        <Insets bottom="18.0" />
                     </VBox.margin>
                  </SplitPane>
               </children>
               <VBox.margin>
                  <Insets left="150.0" />
               </VBox.margin>
            </VBox>
            <HBox alignment="BASELINE_RIGHT">
            <Button fx:id="submitNewDataButton" mnemonicParsing="false" onAction="#submit" text="Soumettre">
                  <padding>
                     <Insets bottom="13.0" left="13.0" right="13.0" top="13.0" />
                  </padding></Button>
               <padding>
                  <Insets bottom="100.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </content>
</fx:root>
